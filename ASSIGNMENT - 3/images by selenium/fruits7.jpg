        P.when("A", "jQuery").execute(function(A, $) {
            $("#selectQuantity [name='quantity'], #mobileQuantityDropDown").live("change", function (event) {

                if (event.updateDDMOnQuantityChange) {
                    return;
                }
                event.updateDDMOnQuantityChange = 1;

                // "#buybox" is included in this list because if there is no accordion row, then it is a single-offer layout
                var accordionRow = $(this).closest('[id$="AccordionRow"], #buybox');

                var quantity = $(this).val();
                var ddmQrWorkflowFixFlag = true;
                var asin;
                var countryCode = "IN".toLowerCase();
                var refreshAccordion = true;

                asin = accordionRow.find("#ddmSelectAsin").val();

                if(countryCode == 'in' && ddmQrWorkflowFixFlag) {
                    if(typeof asin == 'undefined' || asin == "" || asin == null){
                        asin = "B00VBOQ2PQ";
                        refreshAccordion = false;
                    }
                }

                var deviceType = "web";

                if (!asin || !quantity) {
                    return;
                }

                var canReload = false;
                if (typeof sbrContext !== 'undefined') {
                    canReload = sbrContext.sbrBehavior && sbrContext.sbrBehavior.reloadPageWhenBBWinnerChange;
                }
                var merchantId = "";
                var preselectedMerchantId = "";

                if(canReload) {
                    preselectedMerchantId = accordionRow.find("#ddmSelectPreselectedMerchant").val();
                    if(countryCode == 'in' && ddmQrWorkflowFixFlag) {
                        if(typeof preselectedMerchantId == 'undefined' || preselectedMerchantId == null){
                            preselectedMerchantId = "A13YI9Q2I9D70B";
                        }
                    }
                } else {
                    merchantId = accordionRow.find("#ddmSelectMerchant").val();
                    if(countryCode == 'in' && ddmQrWorkflowFixFlag) {
                        if(typeof merchantId == 'undefined' || merchantId == null){
                            merchantId = "A13YI9Q2I9D70B";
                        }
                    }
                }

                var ddmQuantityRefreshMigrationFlag =  true;
                if (ddmQuantityRefreshMigrationFlag) {

                    var params = [];
                    params.push("asin=" + asin);
                    params.push("quantity=" + quantity);
                    params.push("exclusiveMerchantId=" + merchantId);
                    params.push("merchantId=" + merchantId);
                    params.push("preselectedMerchantId=" + preselectedMerchantId);
                    params.push("deviceType=" + deviceType);
                    params.push("clientId=retailwebsite");
                    params.push("showFeatures=dynamicDeliveryMessage");
                    params.push("ie=UTF8");
                    params.push("experienceId=dynamicDeliveryMessageQuantityRefreshAjaxExperience");

                    $.ajax({
                        type: "GET",
                        url: "/gp/product/ajax?",
                        contentType: 'application/x-www-form-urlencoded;charset=utf-8',
                        data: params.join('&'),
                        accordionRow: accordionRow,
                        dataType: "html",
                        success: function (objResponse) {
                            if (objResponse != null && objResponse != "") {
                                if (countryCode == 'in' && !refreshAccordion && ddmQrWorkflowFixFlag) {
                                    $("#dynamicDeliveryMessage").replaceWith(objResponse);
                                } else {
                                    accordionRow.find("#dynamicDeliveryMessage").replaceWith(objResponse);
                                }
                            }
                        }
                    });

                } else {

                    $.ajax({
                        type: "POST",
                        url: "/gp/product/features/dynamic-delivery-message/udp-ajax-handler/get-delivery-message.html?_encoding=UTF-8",
                        contentType: 'application/x-www-form-urlencoded;charset=utf-8',
                        data: {
                            "asin": asin,
                            "deviceType": deviceType,
                            "merchantId": merchantId,
                            "preselectedMerchantId": preselectedMerchantId,
                            "quantity": quantity
                        },
                        accordionRow: accordionRow,
                        dataType: "html",
                        success: function(objResponse) {
                            if (objResponse != null && objResponse != "") {
                                if(countryCode == 'in' && !refreshAccordion && ddmQrWorkflowFixFlag) {
                                    $("#dynamicDeliveryMessage").replaceWith(objResponse);
                                } else{
                                    accordionRow.find("#dynamicDeliveryMessage").replaceWith(objResponse);
                                }
                            }
                        }
                    });

                }

            });
        });
